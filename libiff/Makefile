install_target: first FORCE
	@test -d $(INSTALL_ROOT)/home/chris/Dropbox/Dev/AIE/libiff/./lib || mkdir -p $(INSTALL_ROOT)/home/chris/Dropbox/Dev/AIE/libiff/./lib
